<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<MithraObject xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              objectType="transactional"
              xsi:noNamespaceSchemaLocation="reladomoobject.xsd">

    <PackageName>com.itavery.forecast.domain.mithra.product</PackageName>
    <ClassName>ProductForecastDB</ClassName>
    <DefaultTable>forecaster.product_forecast</DefaultTable>

    <AsOfAttribute name="processingDate"
                   infinityDate="[com.gs.fw.common.mithra.util.DefaultInfinityTimestamp.getDefaultInfinity()]"
                   defaultIfNotSpecified="[com.gs.fw.common.mithra.util.DefaultInfinityTimestamp.getDefaultInfinity()]"
                   fromColumnName="in_date"
                   toColumnName="out_date"
                   isProcessingDate="true"
                   toIsInclusive="false"/>


    <Attribute name="productForecastId" javaType="int" columnName="product_forecast_id" primaryKey="true"
               primaryKeyGeneratorStrategy="SimulatedSequence">
        <SimulatedSequence sequenceName="ProductForecastSequence"
                           sequenceObjectFactoryName="com.itavery.forecast.domain.mithra.SequenceFactory"
                           hasSourceAttribute="false"
                           batchSize="1"
                           initialValue="1"
                           incrementSize="1"/>
    </Attribute>

    <Attribute name="sku" javaType="int" columnName="sku" nullable="false"/>
    <Attribute name="nationalInventory" javaType="int" columnName="national_inventory" nullable="false"/>
    <Attribute name="leadTime" javaType="int" columnName="lead_time" nullable="false"/>
    <Attribute name="inTransitQty" javaType="int" columnName="in_transit_qty" nullable="false"/>
    <Attribute name="forecastThreeMonths" javaType="int" columnName="forecast_three_months" nullable="false"/>
    <Attribute name="forecastSixMonths" javaType="int" columnName="forecast_six_months" nullable="false"/>
    <Attribute name="forecastNineMonths" javaType="int" columnName="forecast_nine_months" nullable="false"/>
    <Attribute name="salesOneMonth" javaType="int" columnName="sales_one_month" nullable="false"/>
    <Attribute name="salesThreeMonths" javaType="int" columnName="sales_three_months" nullable="false"/>
    <Attribute name="salesSixMonths" javaType="int" columnName="sales_six_months" nullable="false"/>
    <Attribute name="salesNineMonths" javaType="int" columnName="sales_nine_months" nullable="false"/>
    <Attribute name="minBank" javaType="int" columnName="min_bank" nullable="false"/>
    <Attribute name="potentialIssues" javaType="String" columnName="potential_issues" maxLength="64" nullable="false"/>
    <Attribute name="piecesPastDue" javaType="int" columnName="pieces_past_due" nullable="false"/>
    <Attribute name="perfSixMonthAvg" javaType="double" columnName="perf_six_month_avg" nullable="false"/>
    <Attribute name="perfTwelveMonthAvg" javaType="double" columnName="perf_twelve_month_avg" nullable="false"/>
    <Attribute name="localBoQty" javaType="int" columnName="local_bo_qty" nullable="false"/>
    <Attribute name="deckRisk" javaType="String" columnName="deck_risk" maxLength="64" nullable="false"/>
    <Attribute name="oeConstraint" javaType="String" columnName="oe_constraint" maxLength="64" nullable="false"/>
    <Attribute name="ppapRisk" javaType="String" columnName="ppap_risk" maxLength="64" nullable="false"/>
    <Attribute name="stopAutoBuy" javaType="String" columnName="stop_auto_buy" maxLength="64" nullable="false"/>
    <Attribute name="revStop" javaType="String" columnName="rev_stop" maxLength="64" nullable="false"/>
    <Attribute name="backorder" javaType="String" columnName="backorder" maxLength="64" nullable="false"/>
    <Attribute name="userId" javaType="int" columnName="user_id" primaryKey="true"/>


</MithraObject>
