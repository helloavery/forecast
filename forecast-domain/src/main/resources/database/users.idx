alter table users add constraint users_pk primary key (user_id, out_date);

drop index if exists users_idx0;
create unique index users_idx0 on users(user_id, email, username, out_date);

drop index if exists users_idx1;
create unique index users_idx1 on users(username, out_date);

drop index if exists users_idx2;
create unique index users_idx2 on users(email, out_date);

